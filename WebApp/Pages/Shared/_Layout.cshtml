<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <base href="/" />
    <title>@ViewData["Title"] - WebApp</title>
   @await Html.PartialAsync("_HeaderPartial")
</head>
<body>


    <nav class="navbar navbar-expand-lg navbar-light bg-light">
        <div class="container-fluid">

            <a class="navbar-brand" href="#">
                <img src="/Imagenes/icono.jfif" class="d-inline-block align-text-top">
            </a>


            <a class="navbar-brand" href="#">Gestion de Parametros</a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
                <div class="navbar-nav">
                    <a class="nav-link" asp-page="/Parametro/Grid">Parametro</a>
                </div>
            </div>
        </div>
    </nav>




    @RenderBody()

    @await Html.PartialAsync("_ScriptsPartial")

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
